{
  "manifest_version": 2,
  "name": "TwitchClipsDownload",
  "short_name": "Twitch Clips",
  "default_locale": "en",
  "version": "0.0.2",
  "description": "Download Twitch Clips",
  "content_scripts": [
    {
      "matches": [
        "*://clips.twitch.tv/*"
      ],
      "js": [
        "index.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
